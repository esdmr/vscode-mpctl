{
  "name": "mpctl",
  "displayName": "MPRIS Media Control",
  "version": "0.1.0",
  "description": "Control any media player that supports MPRIS",
  "categories": [
    "Other"
  ],
  "keywords": [
    "Media",
    "Music",
    "Player",
    "D-Bus",
    "MPRIS",
    "MediaPlayer2",
    "Audacious",
    "Brave",
    "Chromium",
    "Cmus",
    "Feishin",
    "Firefox",
    "gmusicbrowser",
    "Guayadeque",
    "KDE Connect",
    "mpv",
    "ncspot",
    "Plattenalbum",
    "Quod Libet",
    "Resonance",
    "Spotify",
    "Spotube",
    "Telegram",
    "Tidal HiFi",
    "VLC"
  ],
  "homepage": "https://github.com/esdmr/vscode-mpctl#readme",
  "bugs": "https://github.com/esdmr/vscode-mpctl/issues",
  "repository": "https://github.com/esdmr/vscode-mpctl",
  "license": "MIT",
  "author": "Saeed M Rad <esdmr0@gmail.com> (https://github.com/esdmr)",
  "publisher": "esdmr",
  "main": "dist/index.js",
  "scripts": {
    "bundle": "tsup",
    "package": "vsce package"
  },
  "contributes": {
    "commands": [
      {
        "category": "MPRIS Media Control",
        "command": "mpctl.next",
        "icon": "$(debug-continue)",
        "shortTitle": "Next",
        "title": "Next Track"
      },
      {
        "category": "MPRIS Media Control",
        "command": "mpctl.pause",
        "icon": "$(debug-pause)",
        "shortTitle": "Pause",
        "title": "Pause Media"
      },
      {
        "category": "MPRIS Media Control",
        "command": "mpctl.play_pause",
        "icon": "$(debug-start)",
        "shortTitle": "Play/Pause",
        "title": "Toggle Media Play/Pause"
      },
      {
        "category": "MPRIS Media Control",
        "command": "mpctl.play",
        "icon": "$(debug-start)",
        "shortTitle": "Play",
        "title": "Play Media"
      },
      {
        "category": "MPRIS Media Control",
        "command": "mpctl.previous",
        "icon": "$(debug-reverse-continue)",
        "shortTitle": "Previous",
        "title": "Previous Track"
      },
      {
        "category": "MPRIS Media Control",
        "command": "mpctl.reconnect",
        "icon": "$(debug-restart)",
        "shortTitle": "Reconnect",
        "title": "Reconnect to D-Bus"
      },
      {
        "category": "MPRIS Media Control",
        "command": "mpctl.stop",
        "icon": "$(debug-stop)",
        "shortTitle": "Stop",
        "title": "Stop Media"
      },
      {
        "category": "MPRIS Media Control",
        "command": "mpctl.switch",
        "icon": "$(run-all)",
        "shortTitle": "Switch",
        "title": "Switch Player"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "dependencies": {
    "dbus-ts": "0.0.7",
    "jimp": "1.6.0",
    "mime-sniffer": "0.0.3"
  },
  "devDependencies": {
    "@dbus-types/dbus": "0.0.4",
    "@types/node": "20.17.30",
    "@types/vscode": "1.99.0",
    "@vscode/vsce": "3.3.2",
    "tsup": "8.4.0",
    "webpack": "npm:ignoredep@200.9999.0",
    "xo": "0.60.0"
  },
  "engines": {
    "node": ">=20",
    "vscode": "^1.99.0"
  },
  "preview": true,
  "pnpm": {
    "ignoredOptionalDependencies": [
      "@vscode/*"
    ]
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
